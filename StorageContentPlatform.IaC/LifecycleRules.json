{
  "rules": [
    {
      "enabled": true,
      "name": "InventoryRule",
      "type": "Lifecycle",
      "definition": {
        "actions": {
          "baseBlob": {
            "delete": {
              "daysAfterCreationGreaterThan": 10
            }
          }
        },
        "filters": {
          "blobTypes": [
            "blockBlob"
          ],
          "prefixMatch": [
            "inventory/"
          ]
        }
      }
    },
    {
      "enabled": true,
      "name": "RemoteDocumentsLifecycleRule",
      "type": "Lifecycle",
      "definition": {
        "actions": {
          "baseBlob": {
            "tierToCold": {
              "daysAfterCreationGreaterThan": 30
            },
            "tierToCool": {
              "daysAfterCreationGreaterThan": 10
            },
            "delete": {
              "daysAfterCreationGreaterThan": 90
            }
          }
        },
        "filters": {
          "blobTypes": [
            "blockBlob"
          ],
          "prefixMatch": [
            "scpwusdocuments/"
          ]
        }
      }
    },
    {
      "enabled": true,
      "name": "LocalDocumentsLifecycleRule",
      "type": "Lifecycle",
      "definition": {
        "actions": {
          "baseBlob": {
            "tierToCold": {
              "daysAfterCreationGreaterThan": 30
            },
            "tierToCool": {
              "daysAfterCreationGreaterThan": 10
            },
            "delete": {
              "daysAfterCreationGreaterThan": 90
            }
          }
        },
        "filters": {
          "blobTypes": [
            "blockBlob"
          ],
          "prefixMatch": [
            "scpweudocuments/"
          ]
        }
      }
    }
  ]
}